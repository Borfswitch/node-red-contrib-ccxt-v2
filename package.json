{
  "name": "node-red-contrib-ccxt-v2",
  "version": "0.2.3",
  "description": "node-red ccxt v2.0 is a fork of node-red-contrib-ccxt with numberous updates supporting all exchanges and APIs.",
  "keywords": [
    "node-red",
    "ccxt",
    "bitcoin",
    "cryptocurrency",
    "trading",
    "cryptoexchange",
    "cryptotrading"
  ],
  "scripts": {
    "maps": "genmaps --sourceRoot /ccxt-v2/",
    "debug": "genmaps -r /ccxt-v2/ && node --inspect-brk=9229 /usr/local/lib/node_modules/node-red/red.js",
    "start": "/usr/local/lib/node_modules/node-red/red.js",
    "test": "mocha \"test/**/*_spec.js\"",
    "lint": "eslint"
  },
  "author": "Neil Lyon-S",
  "license": "Apache-2.0",
  "dependencies": {
    "ccxt": "^1.41.89"
  },
  "node-red": {
    "nodes": {
      "ccxt-api-v2": "ccxt-v2/ccxt-api.js"
    }
  },
  "devDependencies": {
    "ansicolor": "^1.1.93",
    "as-table": "^1.0.55",
    "ololog": "^1.1.164"
  }
}
